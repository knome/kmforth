@ "standard" ;

:main { $hello $world }
  "hello" $hello.set
  "world" $world.set
  
  $hello.get print
  $world.get print
  
  # { $hello.get }
  
  # 1
  # {
  #   $hello.get print
  # }
  # {
  #   $world.get print
  # }
  # if
;
